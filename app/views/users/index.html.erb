<h1>Users</h1>
user#index


<%= render 'books/list', user: @user, book: @book %>

<table>
    <th>image</th>
    <th>name</th>
  </tr>

  <% @users.each do |user| %>
    <tr>
      <td><%= image_tag user.get_profile_image(100,100) %></td>
      <td>
         <%= user.name %>
      </td>
      <td>
        <%= link_to user_path(user.id) do %>
          Show
        <% end %>
      </td>
    </tr>
  <% end %>
</table>